p = float(input(f"Quer saber seu IMC? Digite seu Peso: "))
a = float(input(f"Qual a sua altura? "))
IMC = p / (a**2)
if IMC < 18.5:
    print(f"Seu IMC é de {IMC:.2f}, você está magro, engorde")
elif IMC > 25:
    print(f"Seu IMC é de {IMC:.2f}, você está obeso, emagreça")
else:
    print(f"Seu IMC é de {IMC:.2f}, você está no peso ideal")
