nota = float(input(f"De 0 รก 10, qual a nota do aluno? "))
if nota < 6:
    print(f"Nota F")
elif nota >= 6 and nota < 6.9:
    print(f"Nota D")
elif nota >= 7 and nota < 7.9:
    print(f"Nota C")
elif nota >= 8 and nota < 8.9:
    print(f"Nota B")
elif nota >= 9 and nota < 9.9:
    print(f"Nota A")
elif nota == 10:
    print(f"Nota A+")
else:
    print(f"Erro, tente novamente.")
